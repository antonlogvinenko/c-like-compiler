
int main()
{
	int digit = 0;
	printf("enter a digit:\n");
	scanf("%i",digit);
	switch(digit)
	{
		case 0: case 99: { printf("zero");
			break; }
		case 1: { printf("one");
			break; }
		case 2: { printf("two");
			break; }
		case 3: { printf("three");
			break; }
		case 4: { printf("four");
			break; }
		default: {
			printf("not a digit\n");
			break; }
		case 5: { printf("five");
			break; }
		case 6: { printf("six");
			break; }
		case 7: { printf("seven");
			break; }
		case 8: { printf("eight");
			break; }
		case 9: { printf("nine");
			break; }       
		
			                          
	}


}